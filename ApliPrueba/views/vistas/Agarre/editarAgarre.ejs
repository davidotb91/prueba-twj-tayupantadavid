<div class="row">
    <div class="col-sm-8">
        <form action="agarre/editarAgarre" method="post">
            <div class="form-group">
                <label>Nombre</label>
                <input required type="text" class="form-control" placeholder="Nombre" name="nombre">
            </div>

            <div class="form-group">
                <label>Veces</label>
                <input required type="number" class="form-control" placeholder="veces" name="veces">
            </div>

            <div class="form-group">
                <label>Dinero Gastado</label>
                <input required type="number" step="0.01" class="form-control" placeholder="dineroGastado" name="dineroGastado">
            </div>
            <label for="">Usuario</label>

            <select class="form-control" name="idUsuario">
                <% for(var i=0; i<usuarios.length; i++){ %>
                    <option value='<%=usuarios[i].id%>'>
                        <%=usuarios[i].nombres%>
                    </option>
                    <%  } %>
            </select>
            <input type="hidden" name="id" value="<%=agarre.id%>">
            <button type="submit" class="btn btn-success">Editar Agarre</button>
            <br>

        </form>
    </div>
    <div class="col-sm-4">

        <h3 class="text-center"><%=agarre.id%> - <%=agarre.nombre%></h3>
        <h4 class="text-center"> <%=agarre.veces%></h4>
        <h4 class="text-center"> <%=agarre.dineroGastado%></h4>
        <h4 class="text-center"> <%=agarre.idUsuario%></h4>


    </div>
</div>


</div>










</form>
</div>



</div>